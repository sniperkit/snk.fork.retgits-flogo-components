{
  "name": "dynamodbquery",
  "type": "flogo:activity",
  "ref": "github.com/sniperkit/snk.fork.retgits-flogo-components/activity/dynamodbquery",
  "version": "0.2.1",
  "title": "DynamoDB Query",
  "description": "Query objects from Amazon DynamoDB",
  "author": "retgits",
  "homepage": "https://github.com/sniperkit/snk.fork.retgits-flogo-components/tree/master/activity/dynamodbquery",
  "inputs":[
    {
      "name": "awsAccessKeyID",
      "type": "string",
      "required": false
    },
    {
      "name": "awsSecretAccessKey",
      "type": "string",
      "required": false
    },
    {
      "name": "awsRegion",
      "type": "string",
      "required": true
    },
    {
      "name": "dynamoDBTableName",
      "type": "string",
      "required": true
    },
    {
      "name": "dynamoDBIndexName",
      "type": "string",
      "required": false
    },
    {
      "name": "dynamoDBKeyConditionExpression",
      "type": "string",
      "required": true
    },
    {
      "name": "dynamoDBFilterExpression",
      "type": "string"
    },
    {
      "name": "dynamoDBExpressionAttributes",
      "type": "any",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "result",
      "type": "any"
    },
    {
      "name": "scannedCount",
      "type": "string"
    },
    {
      "name": "consumedCapacity",
      "type": "double"
    }
  ]
}
